!To reread your .Xresources file, and throw away your old resources:
!xrdb ~/.Xresources
!To reread your .Xresources file, and keep your old resources:
!xrdb -merge ~/.Xresources

! define some colourschemes

! Solarized color scheme for the X Window System
! http://ethanschoonover.com/solarized
! Common
#define Solarized_yellow       #b58900
#define Solarized_orange       #cb4b16
#define Solarized_red          #dc322f
#define Solarized_magenta      #d33682
#define Solarized_violet       #6c71c4
#define Solarized_blue         #268bd2
#define Solarized_cyan         #2aa198
#define Solarized_green        #859900

! Dark
#define Solarized_dark_base03  #002b36
#define Solarized_dark_base02  #073642
#define Solarized_dark_base01  #586e75
#define Solarized_dark_base00  #657b83
#define Solarized_dark_base0   #839496
#define Solarized_dark_base1   #93a1a1
#define Solarized_dark_base2   #eee8d5
#define Solarized_dark_base3   #fdf6e3

! Light
#define Solarized_light_base03 #fdf6e3
#define Solarized_light_base02 #eee8d5
#define Solarized_light_base01 #93a1a1
#define Solarized_light_base00 #839496
#define Solarized_light_base0  #657b83
#define Solarized_light_base1  #586e75
#define Solarized_light_base2  #073642
#define Solarized_light_base3  #002b36

! Molokai
#define Molokai_black        #1B1D1E
#define Molokai_red          #B60050
#define Molokai_red2         #FF0000
#define Molokai_green        #88BB22
#define Molokai_brown        #C47F2C
#define Molokai_blue         #30309B
#define Molokai_purple       #7E40A5
#define Molokai_cyan         #4699AF
#define Molokai_light_grey   #9999AA
#define Molokai_dark_grey    #303030
#define Molokai_light_red    #F92672
#define Molokai_light_green  #80FF00
#define Molokai_yellow       #FFBA68
#define Molokai_light_blue   #7070F0
#define Molokai_light_purple #BB88DD
#define Molokai_light_cyan   #66D9EF
#define Molokai_white        #F8F8F2

! Lucius theme, light
#define Lucius_black        #626262
#define Lucius_red          #af0000
#define Lucius_red2         #FF0000
#define Lucius_green        #008700
#define Lucius_brown        #af8700
#define Lucius_blue         #87afd7
#define Lucius_purple       #870087
#define Lucius_cyan         #0087af
#define Lucius_light_grey   #9e9e9e
#define Lucius_dark_grey    #8a8a8a
#define Lucius_light_red    #d70000
#define Lucius_light_green  #00af00
#define Lucius_yellow       #ffaf00
#define Lucius_light_blue   #0087d7
#define Lucius_light_purple #af00af
#define Lucius_light_cyan   #00afd7
#define Lucius_white        #eeeeee

